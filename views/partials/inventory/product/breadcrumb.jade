.breadcrumb-triangle.z-depth-1
  a(ui-sref="inventory.product", ui-sref-opts="{ reload: true, inherit: false }", ui-sref-active-eq="active")
    span.item-icon.fa.fa-list
    span.item-text List
  a(ng-if="searchVM.state.includes('inventory.product.detail')", ui-sref="inventory.product.detail", ui-sref-opts="{ reload: true }", ui-sref-active="active")
    span.item-icon.fa.fa-binoculars
    span.item-text Details