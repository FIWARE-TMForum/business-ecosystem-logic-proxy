.breadcrumb-triangle.z-depth-1
  a(ui-sref="inventory.productOrder", ui-sref-opts="{ reload: true, inherit: false }", ui-sref-active-eq="active")
    span.item-icon.fa.fa-list
    span.item-text List
  a.active(ng-if="searchVM.state.includes('inventory.productOrder.detail')", ui-sref="inventory.productOrder.detail", ui-sref-opts="{ reload: true }")
    span.item-icon.fa.fa-binoculars
    span.item-text Details